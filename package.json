    "test:Firefox": "npx playwright test --project=firefox --headed",
    "test:Webkit": "npx playwright test --project=webkit --headed",
    "test:Parallel": "npx playwright test --workers=3",
    "test:Serial": "npx playwright test --workers=1",
    "test:Sanity": "npx playwright test --grep @sanity",
    "test:Regression": "npx playwright test --grep @regression"
